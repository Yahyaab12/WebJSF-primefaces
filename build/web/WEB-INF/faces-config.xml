<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>chartController</managed-bean-name>
        <managed-bean-class>domain.ChartController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>machineController</managed-bean-name>
        <managed-bean-class>domain.MachineController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        
    </managed-bean>
    <managed-bean>
    <managed-bean-name>languageBean</managed-bean-name>
    <managed-bean-class>domain.LanguageBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
<navigation-rule>
    <from-view-id>/template.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>success</from-outcome>
        <to-view-id>/template.xhtml</to-view-id>
        <redirect />
    </navigation-case>
</navigation-rule>


</faces-config>
